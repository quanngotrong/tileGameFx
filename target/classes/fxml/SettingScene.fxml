<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@../css/SettingScene.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fitHeight="400.0" fitWidth="600.0">
         <image>
            <Image url="@../css/image/pause-scene.png" />
         </image>
      </ImageView>
      <Button fx:id="soundOn" layoutX="239.0" layoutY="128.0" mnemonicParsing="false" prefHeight="62.0" prefWidth="126.0" style="-fx-background-color: transparent;">
         <graphic>
            <ImageView fitHeight="44.0" fitWidth="102.0">
               <image>
                  <Image url="@../css/image/on.png" />
               </image>
            </ImageView>
         </graphic></Button>
      <Button fx:id="soundOff" layoutX="418.0" layoutY="128.0" mnemonicParsing="false" prefHeight="62.0" prefWidth="126.0" style="-fx-background-color: transparent;">
         <graphic>
            <ImageView fitHeight="46.0" fitWidth="103.0">
               <image>
                  <Image url="@../css/image/off.png" />
               </image>
            </ImageView>
         </graphic></Button>
      <Button fx:id="back" layoutX="28.0" layoutY="309.0" mnemonicParsing="false" prefHeight="61.0" prefWidth="93.0" />
      <Button fx:id="reset" layoutX="244.0" layoutY="303.0" mnemonicParsing="false" prefHeight="62.0" prefWidth="241.0" style="-fx-background-color: transparent;">
         <graphic>
            <ImageView fitHeight="50.0" fitWidth="222.0">
               <image>
                  <Image url="@../css/image/reset.png" />
               </image>
            </ImageView>
         </graphic></Button>
   </children>
</AnchorPane>
